{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@supabase/supabase-js": "^2.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vercel/speed-insights": "^1.2.0",
    "antd": "^5.28.1",
    "dayjs": "^1.11.19",
    "i18next": "^23.16.8",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^7.9.5",
    "react-scripts": "5.0.1",
    "recharts": "^3.3.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node --max-old-space-size=4096 ./node_modules/react-scripts/scripts/build.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n:csv-to-json": "node scripts/csv-to-json.js",
    "i18n:verify": "node scripts/verify-translations.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings 10",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-router|react-router-dom|@remix-run)/)"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1"
  }
}
